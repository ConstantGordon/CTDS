                    <colspec colnum="1" colname="col1" colwidth="5%" rowheader="headers" class="- topic/colspec " xtrf="file:/C:/Users/Constant/Documents/GitHub/Content/CTDS/English/Topics/LanguageProficiency.dita" xtrc="colspec:1;17:71"/>
